{
   "extends": "tslint-config-standard",
   "rules": {
      "indent": [true, "spaces"],
      "ter-indent": [true, 3],
      "semicolon": [true, "always"],
      "no-floating-promises": false,
      "await-promise": false,
      "triple-equals": false,
      "no-empty": false,
      "deprecation": false,
      "variable-name": false,
      "new-parens": false,
      "no-angle-bracket-type-assertion": false,
      "curly": [true, "ignore-same-line"],
      "brace-style": false,
      "one-line": false,
      "space-before-function-paren": [true, "never"]
   }
}